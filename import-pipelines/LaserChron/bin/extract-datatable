#!/usr/bin/env python

from sys import argv, stdout
from click import echo
from pandas import read_excel

infile = argv[1]

echo(infile, err=True)

try:
    outfile = argv[2]
except IndexError:
    outfile = stdout

df = read_excel(infile, sheet_name="datatable", header=None)
df.to_csv(outfile, compression='gzip')
